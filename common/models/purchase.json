{
  "name": "Order",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "quantity": {
      "type": "number",
      "required": true
    },
    "price": {
      "type": "number",
      "required": true,
      "default": 0.0,
      "postgresql": {
        "dataType": "decimal",
        "dataPrecision": 10,
        "dataScale": 2
      }
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
