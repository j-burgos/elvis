{
  "name": "product",
  "plural": "products",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "likes": {
      "type": "hasAndBelongsToMany",
      "model": "user"
    },
    "inventory": {
      "type": "hasMany",
      "model": "user",
      "foreignKey": "productId",
      "through": "inventory"
    },
    "prices": {
      "type": "hasMany",
      "model": "price",
      "foreignKey": "productId",
      "through": "price"
    }
  },
  "acls": [],
  "methods": {}
}
