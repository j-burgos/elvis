{
  "name": "Product",
  "plural": "products",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "stock": {
      "type": "number",
      "required": true
    },
    "price": {
      "type": "number",
      "required": true,
      "postgresql": {
        "dataType": "decimal",
        "dataPrecision": 10,
        "dataScale": 2
      }
    }
  },
  "validations": [],
  "relations": {
    "orders": {
      "type": "hasMany",
      "model": "user",
      "foreignKey": "productId",
      "through": "Inventory"
    },
    "likes": {
      "type": "hasAndBelongsToMany",
      "model": "user",
      "foreignKey": "productId"
    }
  },
  "acls": [
  ],
  "methods": {
  }
}
